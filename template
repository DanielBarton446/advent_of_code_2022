fn read_input(input: &str) -> &str {
    ""
}

fn main() {
    let input = &read_input("../../inputs/DAY_INPUT");
}

mod p1 {
    pub fn solve(input: &str) -> usize {
       0 
    }
}

mod p2 {
    pub fn solve(input: &str) -> usize {
       0 
    }
}

#[cfg(test)]
mod day04_tests {
    use super::*;
    
    const DAY_INPUT: &str = include_str!("../../inputs/DAY_INPUT");
    const SAMPLE: &str = include_str!("../../inputs/SAMPLE");

    #[test]
    fn p1_sample() {
        assert_eq!(p1::solve(SAMPLE), 0)
    }

    #[test]
    #[ignore]
    fn p1_input() {
        let input = &read_input(DAY_INPUT);
        assert_eq!(p1::solve(input), 0)
    }

    #[test]
    #[ignore]
    fn p2_sample() {
        assert_eq!(p2::solve(SAMPLE), 0)
    }

    #[test]
    #[ignore]
    fn p2_input() {
        let input = &read_input(DAY_INPUT);
        assert_eq!(p2::solve(input), 0)
    }

}

